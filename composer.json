{
    "name": "thinktomorrow/trader",
    "description": "Generic logic for a basic shop app",
    "keywords": [
        "thinktomorrow",
        "trader"
    ],
    "homepage": "https://github.com/thinktomorrow/trader",
    "license": "MIT",
    "authors": [
        {
            "name": "Ben Cavens",
            "email": "cavensben@gmail.com"
        }
    ],
    "require": {
        "php" : ">=7.0",
        "moneyphp/money": "^3.0",
        "psr/container": "^1.0",
        "beberlei/assert": "^2.7",
        "winzou/state-machine": "^0.3.2",
        "Illuminate/Contracts": "dev-master@dev",
        "ramsey/uuid": "^3.7"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "mockery/mockery" : "~0.9",
        "league/container": "^2.4",
        "pimple/pimple": "^3.2",
        "GromNaN/pimple-interop": "^1.0",
        "illuminate/container": "dev-master@dev"
    },
    "autoload": {
        "psr-4": {
            "Thinktomorrow\\Trader\\": "src/",
            "App\\":"app/"
        },
      "classmap":[]
    },
  "autoload-dev": {
      "psr-4": {
        "Thinktomorrow\\Trader\\Tests\\": "tests"
      },
      "classmap":[]
  },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
