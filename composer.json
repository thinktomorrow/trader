{
    "name": "thinktomorrow/trader",
    "description": "Generic logic for a basic shop app",
    "keywords": [
        "thinktomorrow",
        "trader"
    ],
    "homepage": "https://github.com/thinktomorrow/trader",
    "license": "MIT",
    "authors": [
        {
            "name": "Ben Cavens",
            "email": "cavensben@gmail.com"
        }
    ],
    "require": {
        "php" : ">=7.3.9",
        "moneyphp/money": "^3.3",
        "psr/container": "^1.0",
        "beberlei/assert": "^3.2",
        "winzou/state-machine": "^0.3.2",
        "illuminate/contracts": "^6.0",
        "illuminate/support": "^6.0",
        "ramsey/uuid": "^3.9",
        "thinktomorrow/magic-attributes": "^0.2"
    },
    "require-dev": {
        "phpunit/phpunit" : "~8.1",
        "scrutinizer/ocular": "~1.4",
        "illuminate/container": "^6.0",
        "phpstan/phpstan": "^0.12.4",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Thinktomorrow\\Trader\\": "src/",
            "Thinktomorrow\\Trader\\Infrastructure": "Infrastructure/"
        },
      "classmap":[]
    },
  "autoload-dev": {
      "psr-4": {
        "Thinktomorrow\\Trader\\Tests\\": "tests/"
      },
      "classmap":[]
  },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
