{
    "name": "thinktomorrow/trader",
    "description": "Generic logic for a Laravel shop app",
    "keywords": [
        "thinktomorrow",
        "trader"
    ],
    "homepage": "https://github.com/thinktomorrow/trader",
    "license": "MIT",
    "authors": [
        {
            "name": "Ben Cavens",
            "email": "cavensben@gmail.com"
        }
    ],
    "require": {
        "php" : ">=8.1",
        "moneyphp/money": "^4.0",
        "beberlei/assert": "^3.3",
        "ext-bcmath": "*",
        "ext-intl": "*"
    },
    "require-dev": {
        "phpunit/phpunit" : "~9.5",
        "scrutinizer/ocular": "~1.4",
        "phpstan/phpstan": "^0.12.4",
        "squizlabs/php_codesniffer": "^3.5",
        "behat/behat": "^3.10"
    },
    "autoload": {
        "psr-4": {
            "Thinktomorrow\\Trader\\": "src/"
        },
      "classmap":[]
    },
  "autoload-dev": {
      "psr-4": {
          "Tests\\": "tests/"
      },
      "classmap":[]
  },
    "scripts": {
        "test": "phpunit"
    }
}
