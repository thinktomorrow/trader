{
    "name": "thinktomorrow/trader",
    "description": "Abstraction to deal with prices and money in a ecommerce environment",
    "keywords": [
        "thinktomorrow",
        "trader"
    ],
    "homepage": "https://github.com/thinktomorrow/trader",
    "license": "MIT",
    "authors": [
        {
            "name": "Ben Cavens",
            "email": "cavensben@gmail.com"
        }
    ],
    "require": {
        "php" : ">=7.0",
        "moneyphp/money": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
       "mockery/mockery" : "~0.9",
        "orchestra/testbench": "3.2|3.3.x@dev"
    },
    "autoload": {
        "psr-4": {
            "Thinktomorrow\\Trader\\": "src/"
        },
      "classmap":[]
    },
  "autoload-dev": {
      "psr-4": {
        "Thinktomorrow\\Trader\\Tests\\": "tests",
        "App\\":"app/"
      },
      "classmap":[]
  },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
